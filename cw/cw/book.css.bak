#slider{
	overflow: hidden; 
	height: 500px;
}

#slider figure{
	position: relative;
	width: 500%; 
	height:500px;
	padding-top: 80px; 
	margin: 0;
	animation: 25s slider infinite; 
	
}
#slider figure img{
	width: 20%;
	height: 500px;
	float: left; 
	object-fit: contain;
	
}
.slide{
	width: 500px;
	margin-left: 200px;
} 

backgroundt{
	background-image: url("bcke.jpg"); 
	background-size:contain;
}
	


@keyframes slider{
	
	20%{
		left: 0;
	}
	25%{
		left: -100%;
	}
	45%{
		left: -100%;
	}
	50%{
		left: -200%;
	}
	70%{
		left: -200%;
	}
	75%{
		left: -300%;
	}
	95%{
		left: -300%;
	}
	100%{
		left: -400%;
	} 
}

